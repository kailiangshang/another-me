# ============================================
# Another Me - 环境变量配置示例
# ============================================

# OpenAI Compatible API 配置
OPENAI_API_KEY=sk-your-api-key-here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-3.5-turbo

# 应用配置
APP_NAME=Another Me
APP_VERSION=1.0.0
DEBUG=false

# 后端配置
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# 前端配置 (开发环境)
VITE_API_BASE_URL=http://localhost:8000

# 数据目录
DATA_DIR=./data

# 向量数据库配置
RAG_VECTOR_STORE_PATH=./data/rag_vector_store
MEM_VECTOR_STORE_PATH=./data/mem_vector_store

# 日志配置
LOG_LEVEL=INFO
LOG_DIR=./data/logs
